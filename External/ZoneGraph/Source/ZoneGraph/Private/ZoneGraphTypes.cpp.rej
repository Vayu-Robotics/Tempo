+bool FZoneLaneProfile::IsValid(const bool bMustHaveName) const
+{
+	if (bMustHaveName && Name == NAME_None)
+	{
+		return false;
+	}
+		
+	if (!ID.IsValid())
+	{
+		return false;
+	}
+		
+	for (const FZoneLaneDesc& Lane : Lanes)
+	{
+		if (Lane.Width <= 0.0f)
+		{
+			return false;
+		}
+	}
+
+	return true;
+}
